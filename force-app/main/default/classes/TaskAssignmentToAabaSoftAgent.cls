    public static AssignedAgentWrapper GetSmallAssignIdAgent(List<AbbaSoft_User__c> AllunssignedUser){
        AssignedAgentWrapper AssignAgentWrap = new AssignedAgentWrapper();
        try{
            map<string, AbbaSoft_User__c> mapofassignedAabasoftuser = new map<String, AbbaSoft_User__c>();
            decimal SmallesassignedId = 0;
            string unassignedAgentID = '';
            string assignedAgentUniqueID = '';
            AbbaSoft_User__c agentTobeAssigned;
            for ( Integer i = 0; i < AllunssignedUser.size(); i++ ){
                if ( i == 0 ){
                    SmallesassignedId = AllunssignedUser[i].Assignment_Id__c;
                    unassignedAgentID = AllunssignedUser[i].id;
                    agentTobeAssigned = AllunssignedUser[i];
                    assignedAgentUniqueID = AllunssignedUser[i].Agent_Unique_Id__c;
                }
                else if ( SmallesassignedId > AllunssignedUser[i].Assignment_Id__c ){
                    SmallesassignedId = AllunssignedUser[i].Assignment_Id__c;
                    unassignedAgentID = AllunssignedUser[i].id;
                    agentTobeAssigned = AllunssignedUser[i];
                    assignedAgentUniqueID = AllunssignedUser[i].Agent_Unique_Id__c;
                }
            }
            //mapofassignedAabasoftuser.put(unassignedAgentID, agentTobeAssigned);
            AssignAgentWrap.assignedAgent = agentTobeAssigned;
            AssignAgentWrap.assignedAgentUniqueID = assignedAgentUniqueID;
            AssignAgentWrap.AgentFoundOrNot = 'Agent Found';
            System.debug('AssignAgentWrap ====> ' + AssignAgentWrap);
            return AssignAgentWrap;

        }
        catch ( Exception e ){
            System.debug('the error has occured =====> ' + e.getStackTraceString());
        }

        return AssignAgentWrap;
    }

    public static void ConvertAgentWithtaskAssigned(AbbaSoft_User__c TobeAssignedAgent){
        System.debug('TobeAssignedAgent ===>' + TobeAssignedAgent);
        if ( TobeAssignedAgent.IsTaskAssigned__c == false ){
            AbbaSoft_User__c taskAssignedAgent = new AbbaSoft_User__c();
            taskAssignedAgent.id = TobeAssignedAgent.id;
            taskAssignedAgent.IsTaskAssigned__c = true;
            update taskAssignedAgent;
        }
    }

    public static decimal getsmallestAssignmentId(List<AbbaSoft_User__c> AllunssignedUser){
        decimal SmallesassignedId = 0;
        try{

            for ( Integer i = 0; i < AllunssignedUser.size(); i++ ){
                if ( i == 0 ){
                    SmallesassignedId = AllunssignedUser[i].Assignment_Id__c;
                }
                else if ( SmallesassignedId > AllunssignedUser[i].Assignment_Id__c ){
                    SmallesassignedId = AllunssignedUser[i].Assignment_Id__c;
                }
            }
            return SmallesassignedId;
        }
        catch ( Exception e ){
            System.debug('the error has occured due to ===> ' + e.getMessage());
        }
        return SmallesassignedId;
    }

